<aura:component
    implements="forceCommunity:themeLayout,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes"
    access="global" controller="EI_SignUpController">	
    
    <ltng:require styles="{!$Resource.TDSTheme+'/assets/css/custom.css'}"
                 scripts="{!join(',',
                           $Resource.TDSTheme+'/assets/js/plugin.min.js',
                           $Resource.TDSTheme+'/assets/js/custom.js',
                          $Resource.TDSTheme+'/assets/js/jquery-3.6.0.js',
                          $Resource.TDSTheme+'/assets/js/jquery-ui.js')}"                  
                  />    
    <aura:attribute name="phoneCodePicklist" type="String[]"/>    
    <aura:attribute name="aggrement" type="Boolean" />
    <aura:attribute name="OrgUser" type="Boolean" default='false' />
    <aura:attribute name="HeadOfficeUser" type="Boolean" default='false' />
    <aura:attribute name="HeadoficeUsercheckbox" type="Boolean" default='false' />
    <aura:attribute name="isModalOpen" type="Boolean" />
    <aura:attribute name="vlidateEmail" type="Boolean" />
    <aura:attribute name="isModalOpenJointLandlord" type="Boolean" />
    <aura:attribute name="firstName" type="String" />
    <aura:attribute name="lastName" type="String" />
    <aura:attribute name="Email" type="String" />
    <aura:attribute name="TelePhone" type="String" />
    <aura:attribute name="phoneCode" type="String" default="+44"/>
    <aura:attribute name="Phone" type="String" />
    <aura:attribute name="Address" type="String" />
    <aura:attribute name="companyName" type="String" />
    <aura:attribute name="companyPhoneCode" type="String" default="+44"/>
    <aura:attribute name="companyPhone" type="String" />
    <aura:attribute name="letAgntId" type="String" />
    <aura:attribute name="companyAddress" type="String" />
    <aura:attribute name="userType" type="String" access="global" default=""/>
    <aura:attribute name="Title" type="String" default=""/>
    <aura:attribute name="flagUserType" type="Boolean"  default="false"/> 
    <aura:attribute name="flagTitle" type="Boolean"  default="false"/>
    <aura:attribute name="backgroundimage" type="String" default="{!$Resource.TDSTheme+'/assets/img/login-banner.png'}"/>

    <aura:attribute name="tenantmobcheck" type="Boolean" default="false" />
    <aura:attribute name="flagRegAsOrg" type="Boolean" default="false" />
    <aura:attribute name="orgUsercheckbox" type="Boolean" default="false" />
    <aura:attribute name="flagAddress" type="Boolean" default="false" />
    <aura:attribute name="companyEmail" type="String" />
    <aura:attribute name="companyDrmEmail" type="String" />
    <aura:attribute name="companyFinanceEmail" type="String" />
    
    <aura:attribute name="companyregistration" type="String" />
    <aura:attribute name="tradingname" type="String" />

    <!-- Address finder   -->
    <aura:attribute access="public" name="Street" type="String" />
    <aura:attribute access="public" name="Town" type="String" />
    <aura:attribute access="public" name="PostCode" type="String" />
    <aura:attribute access="public" name="County" type="String" />
    <aura:attribute access="public" name="Country" type="String" />
	<aura:registerEvent name="showToast" type="force:showToast" />
    
    
    <!-- Label and Placeholder, regex attributes -->
    <aura:attribute name="teleNumber" type="String" default="Telephone number"/>
    <aura:attribute name="teleNumberPlaceholder" type="String" default="Enter telephone number"/>
    <aura:attribute name="regexPattern" type="String" default=""/>
    <aura:attribute name="mismatchPatternErr" type="String" default=""/>
    <aura:attribute name="MessagePhone" type="boolean" default="false"/>
    <aura:attribute name="telePhoneError" type="boolean" default="false"/>
    <aura:attribute name="phonelengthError" type="boolean" default="false"/>
    <aura:attribute name="companyPhonelengthError" type="boolean" default="false"/>
    <lightning:navigation aura:id="navService" />
	
     <!-- BootStrap Errors using Aura:if-->
    <aura:attribute name="mobileFormatError" type="boolean" default="false" />
	<aura:attribute name="firstNameError" type="boolean" default="false" />
	<aura:attribute name="surNameError" type="boolean" default="false" />
	<aura:attribute name="emailError" type="boolean" default="false" />
    <aura:attribute name="selectIamerrorMessage" type="boolean" default="false"/>
	<aura:attribute name="titleErrorMessage" type="boolean" default="false"/>
	<aura:attribute name="mobileError" type="boolean" default="false" />
	<aura:attribute name="addressError" type="boolean" default="false" />
	<aura:attribute name="companyNameError" type="boolean" default="false" />
	<aura:attribute name="companyEmailError" type="boolean" default="false" />
	<aura:attribute name="companyPhoneError" type="boolean" default="false" />
	<aura:attribute name="DRMEmailError" type="boolean" default="false" />
	<aura:attribute name="FinanceEmailError" type="boolean" default="false" />
	<aura:attribute name="LettingAgentNumberError" type="boolean" default="false" />
	<aura:attribute name="companyFinanceEmailMessageError" type="boolean" default="false" />
	<aura:attribute name="emailMessageError" type="boolean" default="false" />
	<aura:attribute name="companyDrmEmailMessageError" type="boolean" default="false" />
	<aura:attribute name="companyEmailMessageError" type="boolean" default="false" />
    
     <aura:attribute name="companyregnumbererror" type="boolean" default="false" />
     <aura:attribute name="tradingnameerror" type="boolean" default="false" />
  
    	<aura:attribute name="alreadyAccount" type="boolean" default="true" />
		<aura:attribute name="successfullCreateUser" type="boolean" default="true" />
		<aura:attribute name="errorInCreateUser" type="boolean" default="true" />
    <aura:attribute name="emailAlreadyExist" type="boolean" default="true" />
   <aura:attribute name="errorText" type="String" default=""/>
    
       <aura:handler event="aura:waiting" action="{!c.waiting}"></aura:handler>
<aura:handler event="aura:doneWaiting" action="{!c.doneWaiting}"></aura:handler>
    <aura:attribute name="HideSpinner" type="Boolean" default="false"></aura:attribute>
      <aura:attribute name="PageSpinner" type="Boolean" default="true"></aura:attribute>
    
    <!--Error Attribute-->  
     	<aura:attribute name="regPageErrorNew" type="String" />
        <aura:attribute name="titleErrorNew" type="String" />
        <aura:attribute name="firstNameErrorNew" type="String" />
        <aura:attribute name="surNameErrorNew" type="String" />
        <aura:attribute name="emailErrorNew" type="String" />
        <aura:attribute name="addressErrorNew" type="String" />
        <aura:attribute name="phoneErrorNew" type="String" />
        <aura:attribute name="validEmailErrorNew" type="String"/> 
    	<aura:attribute name="errorList" type="Error_Message__mdt[]"/> 
    
    
    <aura:handler access="public" name="init" value="{!this}" action="{!c.doInit}" />
    
    <head>
        <!-- Required meta tags -->
        <meta charset="utf-8"/>
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no"/>
        <title>TDS</title>
    </head>
 
    <aura:renderIf isTrue="{!v.PageSpinner}">
 <div aura:id="ring" class="lds-dual-ring"></div>
           <aura:set attribute="else">
        <body>
        <lightning:notificationsLibrary aura:id="notifLib" />
        <!-- SignUp HTML Start -->
        <section class="login-details signUp">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-md-6">
                        <!-- Login Banner HTML Start -->
                        <div class="login-banner show-desktop" style="{!'background-image:url('+v.backgroundimage+')'}">
                            <div class="login-banner__text">
                               <!-- <h2>Lorem Ipsum Dolor sit</h2>-->
                                 <h2>Sign Up</h2>
                               <!-- <p>Lorem Ipsum has been the industry's standard dummy text ever since the 1500s, when an unknown printer took a galley of type and scrambled it to make a type specimen book. It has survived not only</p>-->
                              <p>To sign up and start using SafeDeposits Scotland online, please complete the form opposite</p>
                            </div>
                        </div>
                        <!-- Login Banner HTML End -->
                    </div>
                    <div class="col-md-6">
                        <!-- SignUp Form HTML Start -->
                    
                        <div  class="signUp">
                            <div class="login-banner-form">
                                   
                                <figure class="logo">
                               <a href="https://xab195c-yuwehs-uwx3yxqtds.azurewebsites.net/">    <img src="{!$Resource.TDSTheme+'/assets/img/Logo_FIles.png'}" alt="TDS Logo"/></a>
                                 </figure>
                                 <aura:if isTrue="{!!v.alreadyAccount}">
                                     <div class="form-row">
                                            <div id="errorMsg" class="form-group col-12 divMsg2" >
                                                {!v.errorText}
    								 
                                         </div>
                                     </div>
                                         </aura:if>
								<aura:if isTrue="{!!v.successfullCreateUser}">
								<div class="form-row">
                                            <div class="form-group col-12 divMsg" >
										Thank you for registering your account with SafeDeposits Scotland. You will receive an email shortly with instructions on how to activate your account.	
								    </div>
                                     </div>
                                         </aura:if>
                              
                                      <aura:if isTrue="{!!v.emailAlreadyExist}">
								<div class="form-row">
                                            <div class="form-group col-12 divMsg2" >
										 {!v.errorText}
								    </div>
                                     </div>
                                         </aura:if>
                                 <aura:if isTrue="{!!v.errorInCreateUser}">
								<div class="form-row">
                                            <div class="form-group col-12 divMsg" >
										 {!v.errorText}	
								    </div>
                                     </div>
                                         </aura:if>
                              <aura:if isTrue="{!and(v.successfullCreateUser, v.alreadyAccount)}">
                                  <!--<p>Welcome back! Please login to your account.</p>-->
                                <p id="maincon">Please sign up</p>
                                <p class="have-account">Already have an account? <a href="" onclick="{!c.loginPage}">Sign in</a></p>
								
								<aura:if isTrue="{!v.selectIamerrorMessage}">
									 <div id="errorAlert" class="alert alert-danger alert-dismissible fade show">
										<!--<strong>Error!</strong> I am a? is mandatory.-->
                                         {!v.regPageErrorNew}
										<button type="button" class="close" onclick="{!c.hideBootstrapErrors}" name="selectIam" >&times;</button >
									 </div>                 
								</aura:if>
								
								<aura:if isTrue="{!v.titleErrorMessage}">
									 <div id="TitleerrorAlert" class="alert alert-danger alert-dismissible fade show">
										<!--<strong>Error!</strong> Title is mandatory.-->
                                        {!v.titleErrorNew} 
										<button type="button" class="close" onclick="{!c.hideBootstrapErrors}" name="title" >&times;</button >
									 </div>                 
								</aura:if>
								
								<aura:if isTrue="{!v.firstNameError}">
									 <div id="firstNameErrorAlert" class="alert alert-danger alert-dismissible fade show">
										<!--<strong>Error!</strong> First name is mandatory.-->
                                          {!v.firstNameErrorNew}
										<button type="button" class="close" onclick="{!c.hideBootstrapErrors}" name="firstName" >&times;</button >
									 </div>                 
								</aura:if>
								
								<aura:if isTrue="{!v.surNameError}">
									 <div id="SurNameErrorAlert" class="alert alert-danger alert-dismissible fade show">
										<!--<strong>Error!</strong> Surname is mandatory.-->
                                         {!v.surNameErrorNew}
										<button type="button" class="close" onclick="{!c.hideBootstrapErrors}" name="surName" >&times;</button >
									 </div>                 
								</aura:if>
								
								<aura:if isTrue="{!v.emailError}">
									 <div id="EmailErrorAlert" class="alert alert-danger alert-dismissible fade show">
										<!--<strong>Error!</strong> Email is mandatory.-->
                                         {!v.emailErrorNew} 
										<button type="button" class="close" onclick="{!c.hideBootstrapErrors}" name="emailOfUser" >&times;</button >
									 </div>                 
								</aura:if>
								
								<aura:if isTrue="{!v.emailMessageError}">
									 <div id="EmailMessageErrorAlert" class="alert alert-danger alert-dismissible fade show">
										<!--<strong>Error!</strong> Email does not match the allowed pattern.-->
                                          {!v.validEmailErrorNew} 
										<button type="button" class="close" onclick="{!c.hideBootstrapErrors}" name="emailMessage" >&times;</button >
									 </div>                 
								</aura:if>
								<aura:if isTrue="{!v.mobileError}">
									 <div id="MobileNumberErrorAlert" class="alert alert-danger alert-dismissible fade show">
										<strong>Error!</strong>For.
										<button type="button" class="close" onclick="{!c.hideBootstrapErrors}" name="mobileNumber" >&times;</button >
									 </div>                 
								</aura:if>
								
								<aura:if isTrue="{!v.addressError}">
									 <div id="addressErrorAlert" class="alert alert-danger alert-dismissible fade show">
										<!--<strong>Error!</strong> Address is mandatory.-->
                                          {!v.addressErrorNew} 
										<button type="button" class="close" onclick="{!c.hideBootstrapErrors}" name="address" >&times;</button >
									 </div>                 
								</aura:if>
                                  
                                  	<aura:if isTrue="{!v.telePhoneError}">
                                      <div id="telePhoneErrorAlert" class="alert alert-danger alert-dismissible fade show">
                                          <strong>Error!</strong> Please enter valid Landline number or Your entry does not match the allowed pattern.
                                          <button type="button" class="close" onclick="{!c.hideBootstrapErrors}" name="telePhone" >&times;</button >
                                      </div> 
                                  </aura:if>
								
								<aura:if isTrue="{!v.MessagePhone}">
									<div id="validPhoneErrorAlert" class="alert alert-danger alert-dismissible fade show">
										<!--<strong>Error!</strong> Please enter valid phone number or Your entry does not match the allowed pattern.-->
                                        {!v.phoneErrorNew}
										<button type="button" class="close" onclick="{!c.hideBootstrapErrors}" name="phoneNotValid" >&times;</button >
									 </div> 
								</aura:if>
                                  
                                 <aura:if isTrue="{!v.phonelengthError}">
                                      <div id="phonelengthErrorAlert" class="alert alert-danger alert-dismissible fade show">
                                          <!--<strong>Error!</strong> Please enter atleast 11 digit phone number or Your entry does not match the allowed pattern.-->
                                           {!v.phoneErrorNew}
                                          <button type="button" class="close" onclick="{!c.hideBootstrapErrors}" name="phonelength" >&times;</button >
                                      </div> 
                                  </aura:if>
								
								<aura:if isTrue="{!v.companyNameError}">
									<div id="CompanyErrorAlert" class="alert alert-danger alert-dismissible fade show">
									<!--	<strong>Error!</strong> Company name is mandatory.-->
                                        Company name is mandatory.
										<button type="button" class="close" onclick="{!c.hideBootstrapErrors}" name="companyName" >&times;</button >
									 </div> 
								</aura:if>
								
                                <aura:if isTrue="{!v.companyPhonelengthError}">
                                    <div id="companyPhonelengthErrorAlert" class="alert alert-danger alert-dismissible fade show">
                                       <!-- <strong>Error!</strong> Please enter atleast 11 digit company phone number or Your entry does not match the allowed pattern.-->
                                        Please enter atleast 11 digit company phone number or Your entry does not match the allowed pattern.
                                        <button type="button" class="close" onclick="{!c.hideBootstrapErrors}" name="companyPhonelength" >&times;</button >
                                    </div> 
                                </aura:if>
                                  
								<aura:if isTrue="{!v.companyPhoneError}">
									<div id="CompanyPhoneErrorAlert" class="alert alert-danger alert-dismissible fade show">
										<!--<strong>Error!</strong> Company phone is mandatory.-->
                                         Company phone is mandatory.
										<button type="button" class="close" onclick="{!c.hideBootstrapErrors}" name="companyPhone" >&times;</button >
									 </div> 
								</aura:if>
                                  
                                <aura:if isTrue="{!v.companyregnumbererror}">
									<div id="companyregnumbererrorAlert" class="alert alert-danger alert-dismissible fade show">
										<strong>Error!</strong> Company registration number is mandatory.
										<button type="button" class="close" onclick="{!c.hideBootstrapErrors}" name="companyregnumber" >&times;</button >
									 </div> 
								</aura:if>
                                  
                                  <aura:if isTrue="{!v.tradingnameerror}">
									<div id="tradingnameerrorAlert" class="alert alert-danger alert-dismissible fade show">
										<strong>Error!</strong> Trading name is mandatory.
										<button type="button" class="close" onclick="{!c.hideBootstrapErrors}" name="tradingname" >&times;</button >
									 </div> 
								</aura:if>
								
								<aura:if isTrue="{!v.companyEmailError}">
									<div id="CompanyEmailErrorAlert" class="alert alert-danger alert-dismissible fade show">
										<!--<strong>Error!</strong> Company email is mandatory.-->
                                        Company email is mandatory.
										<button type="button" class="close" onclick="{!c.hideBootstrapErrors}" name="companyEmail" >&times;</button >
									 </div> 
								</aura:if>
								
								<aura:if isTrue="{!v.companyEmailMessageError}">
									 <div id="ompanyEmailErrorAlert" class="alert alert-danger alert-dismissible fade show">
										<strong>Error!</strong> Company email does not match the allowed pattern.
										<button type="button" class="close" onclick="{!c.hideBootstrapErrors}" name="companyEmailMessage" >&times;</button >
									 </div>                 
								</aura:if>
								
								<aura:if isTrue="{!v.DRMEmailError}">
									<div id="DRMEmailErrorAlert" class="alert alert-danger alert-dismissible fade show">
										<!--<strong>Error!</strong> DRM email is mandatory.-->
                                        DRM email is mandatory.
										<button type="button" class="close" onclick="{!c.hideBootstrapErrors}" name="DRMEmail" >&times;</button >
									 </div> 
								</aura:if>
								
								<aura:if isTrue="{!v.companyDrmEmailMessageError}">
									 <div id="companyDrmEmailErrorAlert" class="alert alert-danger alert-dismissible fade show">
										<strong>Error!</strong> DRM email does not match the allowed pattern.
										<button type="button" class="close" onclick="{!c.hideBootstrapErrors}" name="DrmEmailMessage" >&times;</button >
									 </div>                 
								</aura:if>
								
								<aura:if isTrue="{!v.FinanceEmailError}">
									<div id="FinanceEmailErrorAlert" class="alert alert-danger alert-dismissible fade show">
										<!--<strong>Error!</strong> Finance email is mandatory.-->
                                        Finance email is mandatory.
										<button type="button" class="close" onclick="{!c.hideBootstrapErrors}" name="FinanceEmail" >&times;</button >
									 </div> 
								</aura:if>
								
								<aura:if isTrue="{!v.companyFinanceEmailMessageError}">
									 <div id="ompanyFinanceEmailErrorAlert" class="alert alert-danger alert-dismissible fade show">
										<strong>Error!</strong> Finance email does not match the allowed pattern.
										<button type="button" class="close" onclick="{!c.hideBootstrapErrors}" name="FinanceEmailMessage" >&times;</button >
									 </div>                 
								</aura:if>
								
								<aura:if isTrue="{!v.LettingAgentNumberError}">
									<div id="LettingAgentNumberEmailErrorAlert" class="alert alert-danger alert-dismissible fade show">
										<!--<strong>Error!</strong> Letting agent number is mandatory.-->
                                        Letting agent number is mandatory.
										<button type="button" class="close" onclick="{!c.hideBootstrapErrors}" name="LettingAgentNumber" >&times;</button >
									 </div> 
								</aura:if>
								
                                <div class="get_In_Touch_form">
                                    <form required="true" >
                                        <div class="form-row">
                                            <div class="form-group col-12">
                                                <label for="selectIam">I am a?</label>
                                                <select  id="selectIam" class="form-control" name="userType" value="{!v.userType}"
                                                        required="true" onchange="{!c.userTypeHadler}" >
                                                    <option >-- Please select ---</option>
                                                    <option value="Agent">Agent</option>
                                                    <option value="Landlord">Landlord</option>	
                                                    <option value="Tenant">Tenant</option>
                                                </select>
                                                
                                            </div>
                                        </div>
                                        
                                        
                                        <aura:if isTrue="{!v.flagRegAsOrg}">
                                            <div class="form-row">
                                                <div class="form-group col-12">
                                                    <div class="form-check">
                                                        <input class="form-check-input" type="checkbox" id="gridCheck1" value="{!v.orgUsercheckbox}" onchange="{!c.OrgUsers}"/>													
                                                        <label class="form-check-label" for="gridCheck1">Registering as an organisation</label>
                                                    </div>
                                                </div>
                                            </div>
                                        </aura:if>
                                        <aura:renderIf isTrue="{!v.aggrement}">
                                            <lightning:input type="checkbox" label="please confirm you have read the joint Landlord information"
                                                             name="aggrenemt_joint_landlord" />
                                        </aura:renderIf>
                                        <aura:renderIf isTrue="{!v.HeadOfficeUser}">
                                            <div class="form-row">
                                                <div class="form-group col-12">
                                                    <div class="form-check">
                                                        <input class="form-check-input" type="checkbox" id="gridCheck2" value="{!v.HeadoficeUsercheckbox}" onchange="{!c.multiplebranches}"/>
                                                        <label class="form-check-label" for="gridCheck2">Do you wish to manage multiple branches through your account?</label>
                                                    </div>
                                                </div>
                                            </div>
                                        </aura:renderIf>
                                        <aura:renderIf isTrue="{!v.OrgUser}">
                                            <aura:renderIf isTrue="{!v.userType == 'Landlord'}">
                                                <lightning:input aura:id="CompfieldId2" label="Company name" name="companyName" value="{!v.companyName}" placeholder="Enter company name"/>
                                                <!--  <lightning:input aura:id="fieldId"
               label="Comapny Address"
               name="companyAddress"
               value="{!v.companyAddress}"
               required="{!v.OrgUser}" />  -->
                                                <div class="form-row">
                                                    <label class="slds-p-top_small">Company phone</label>
                                                    <div class="form-group col-md-4  slds-p-top_large">
                                                        <select id="selectCompanyPhoneCode" class="form-control" value="{!v.companyPhoneCode}"
                                                        onchange="{!c.handleCompanyPhoneCode}" >
                                                            <aura:iteration items="{!v.phoneCodePicklist}" var="itemVar">
                                                                <option text="{!itemVar}" value="{!itemVar}" />
                                                            </aura:iteration>
                                                        </select>
                                                    </div>
                                                    <div class="form-group col-md-8">
                                                        <lightning:input aura:id="CompfieldId2" label="" name="companyPhone" type="tel"
                                                                    value="{!v.companyPhone}" placeholder="Enter company phone"/>
                                                    </div>
                                                </div>
                                                
                                                <lightning:input aura:id="compreg" label="Company registration number" name="companyregistrationnumber" 
                                                                 value="{!v.companyregistration}" placeholder="Enter  company registration number"/>
                                                
                                                <lightning:input aura:id="tradingname" label="Trading Name" name="tradingname" 
                                                                 value="{!v.tradingname}" placeholder="Enter  trading name"/>
                                                <!-- new fields add   -->
                                            </aura:renderIf>
                                            
                                            <aura:renderIf isTrue="{!v.HeadOfficeUser}">
                                                <lightning:input aura:id="CompfieldId" label="Company name" name="companyName" value="{!v.companyName}" placeholder="Enter  company name"/>
                                                <!--  <lightning:input aura:id="fieldId"
               label="Comapny Address"
               name="companyAddress"
               value="{!v.companyAddress}"
               required="{!v.OrgUser}" />  -->
                                                <div class="form-row">
                                                    <label class="slds-p-top_small">Company phone</label>
                                                    <div class="form-group col-md-4  slds-p-top_large">
                                                        <select id="selectCompanyPhoneCode" class="form-control" value="{!v.companyPhoneCode}"
                                                        onchange="{!c.handleCompanyPhoneCode}" >
                                                            <aura:iteration items="{!v.phoneCodePicklist}" var="itemVar">
                                                                <option text="{!itemVar}" value="{!itemVar}" />
                                                            </aura:iteration>
                                                        </select>
                                                    </div>
                                                    <div class="form-group col-md-8">
                                                        <lightning:input aura:id="CompfieldId" label="" name="companyPhone" type="tel"
                                                                    value="{!v.companyPhone}" placeholder="Enter company phone"/>
                                                    </div>
                                                </div>
                                                <lightning:input aura:id="CompfieldId" label="Company email" name="companyEmail" 
                                                                 value="{!v.companyEmail}" placeholder="Enter  company email"/>
												<lightning:input aura:id="CompfieldId" label="Dispute resolution email " name="drmEmail" 
                                                                 value="{!v.companyDrmEmail}" placeholder="Enter dispute resolution email"/>
												 <lightning:input aura:id="CompfieldId" label="Finance email " name="financeEmail "
                                                                 value="{!v.companyFinanceEmail}" placeholder="Enter finance email"/>
