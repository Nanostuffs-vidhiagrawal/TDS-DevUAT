<aura:component implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId" access="global"
                controller="EI_ZD_TenantRespondToClaim">
    <aura:attribute name="ClaimsDetails" type="list" />
    <aura:attribute name="viewClaim" type="Boolean" default="true" />
    <aura:attribute name="isLead" type="boolean" default="false"/>
    <aura:attribute name="showConfirmDiv" type="boolean" default="false"/>
    <aura:attribute name="ViewContinue" type="boolean" default="false"/>
    <aura:attribute name="showConsentError" type="boolean" default="false"/>
    <aura:attribute name="showClaimBreakdown" type="boolean" default="false"/>
    <aura:attribute name="currentItem" type="Integer" default="0" />
    <aura:attribute name="agentResponse" type="boolean" default="true" />
    <aura:attribute name="showAdditionalComments" type="boolean" default="false" />
    <aura:attribute name="showReviewsubmission" type="boolean" default="false" />
    <aura:attribute name="currentClaimCatagories" type="string" default="" />
    <aura:attribute name="showDisputeItemError" type="boolean" default="false" />
    <aura:attribute name="claimId" type="string" default="" />
    <aura:attribute name="isEditable" type="boolean" default="true" />
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <body class="ZD_theme">
        <aura:if isTrue="{!v.viewClaim}">
            <div class="tab-content custom-tab-content" id="sf-tabContent">
                <div class="tab-pane fade show active" id="sf-deposit" role="tabpanel" aria-labelledby="sf-deposit-tab">
                    <!-- Resolution Section HTML Start -->
                    <section class="resolution_section">
                        <div class="container">
                            <div>
                                <aura:if isTrue="{!v.isLead}">
                                    
                                    <aura:if isTrue="{!v.ClaimsDetails[0].TT_respond_evidence_gathering__c}">
                                        <lightning:button variant="brand" label="Review/edit evidence" title="Review/edit evidence"
                                                          onclick="{! c.showContinue}" class="see-all"/>
                                        <aura:set attribute="else">
                                            <lightning:button variant="brand" label="Submit evidence" title="Continue claim journey"
                                                              onclick="{! c.showContinue}" class="see-all"/>
                                        </aura:set>
                                    </aura:if>
                                    <lightning:button variant="brand" label="I agree with the agent/landlord’s claim" title="I agree with the agent/landlord’s claim"
                                                      onclick="{!c.doAgreeToClaim }" class="see-all"/>
                                    <aura:set attribute="else">
                                        <lightning:button variant="brand" label="View Claim" title="Continue claim journey"
                                                          onclick="{! c.showContinue}" class="see-all"/>
                                    </aura:set>
                                </aura:if>
                            </div>
                            <img src="{!$Resource.EI_ZD_Assets  + '/img/resolution.png'}" alt="Resolution" />
                        </div>
                    </section>
                    <!-- Resolution Section HTML End -->
                    <div class="deposit-agent">
                        <div class="container">
                            <div class="row">
                                <div class="col-md-9">
                                    <!-- Table HTML Start -->
                                    <div class="table-container no_navigation ZD_custom_table">
                                        <table class="display scrollbar" style="width:100%">
                                            <tbody>
                                                <tr>
                                                    <td>Address</td>
                                                    <td>{!v.ClaimsDetails[0].Deposit_Account_Number__r.Property_Address__c}</td>
                                                </tr>
                                                <tr>
                                                    <td>Claim status</td>
                                                    <td>{!v.ClaimsDetails[0].Status}</td>
                                                </tr>
                                                
                                                <tr>
                                                    <td>Tenancy start date</td>
                                                    <td> <ui:outputDate value="{!v.ClaimsDetails[0].Deposit_Account_Number__r.Start_Date__c}" format="dd/MM/yyyy"></ui:outputDate></td>
                                                </tr>
                                                <tr>
                                                    <td>Claim number</td>
                                                    <td>{!v.ClaimsDetails[0].External_ID__c}</td>
                                                </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                    <!-- Table HTML End -->
                                    
                                    <!-- Table HTML Start -->
                                    <div class="table-container no_navigation no_space">
                                        <table id="example-2" class="display scrollbar dataTable no-footer" style="width:100%">
                                            <thead>
                                                <tr>
                                                    <th>Name</th>
                                                    <th>Type</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <aura:iteration var="caseParticpnt" items="{!v.ClaimsDetails[0].Case_Participants__r}" indexVar="i">
                                                    <aura:if isTrue="{!caseParticpnt.Type__c =='Tenant'}">
                                                        
                                                        <aura:if isTrue="{!(i+1)%2 == 0}">
                                                            <tr class="even">
                                                                <td> {!caseParticpnt.Contact__r.Name} </td>
                                                                <aura:if isTrue="{!caseParticpnt.Is_Lead__c}">
                                                                    <td> Lead tenant </td>
                                                                    <aura:set attribute="else">
                                                                        <td> Joint tenant </td>
                                                                    </aura:set>
                                                                </aura:if>
                                                                
                                                                <!-- <td> {!caseParticpnt.Type__c}  </td> -->
                                                            </tr>
                                                        </aura:if>
                                                        <aura:if isTrue="{!(i+1)%2 != 0}">
                                                            <tr class="odd">
                                                                <td> {!caseParticpnt.Contact__r.Name} </td>
                                                                <aura:if isTrue="{!caseParticpnt.Is_Lead__c}">
                                                                    <td> Lead tenant </td>
                                                                    <aura:set attribute="else">
                                                                        <td> Joint tenant </td>
                                                                    </aura:set>
                                                                </aura:if>
                                                                <!-- <td> {!caseParticpnt.Type__c}  </td> -->
                                                            </tr>
                                                        </aura:if>
                                                    </aura:if>
                                                </aura:iteration>
                                            </tbody>
                                        </table>
                                    </div>
                                    <!-- Table HTML End -->
                                </div>
                                <div class="col-md-3 info-right-tab">
                                    <p>This page provides a summary of the claim, including where the claim is in the process.</p>
                                    <p>Please progress to review the information and documents that your landlord/agent submitted, and provide the evidence that supports your position.</p>
                                    <p>One tenant must act as lead tenant during the claim process. The lead tenant acts on behalf of the other named tenants, including uploading evidence.</p>
                                    <p>We communicate with all tenants throughout the claim process, but only the lead tenant is able to make decisions on the claim.</p>
                                    <p>If you wish to change the lead tenant, then please contact us via "Need More Help?" below.</p>
                                   <!-- <p class="questionsmark"> <img src="{!$Resource.EI_ZD_Assets  + '/img/question-circle.png'}" alt="Question Mark"/> <a href="#">Need More Help</a>.</p> -->
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                
            </div>
        </aura:if>
        <aura:if isTrue="{!v.showConfirmDiv}">
            <div class="tab-content custom-tab-content" id="sf-tabContent">
                <div class="tab-pane fade show active" id="sf-deposit" role="tabpanel" aria-labelledby="sf-deposit-tab">
                    <div class="deposit-agent">
                        <div class="container">
                            <div class="row">
                                <div class="col-md-9">
                                    <section class="deposit_steps">
                                        <div class="deposit_steps_detail">
                                            <div class="claim-summary_gray-panel border">
                                                <h6>You will need to pay the following</h6>
                                                <div class="form-group row">
                                                    <div class="col-6 col-md-3">
                                                        <label>Amount to be paid</label>
                                                    </div>
                                                    <div class="col-6 col-md-5" style="margin-top: -2%">
                                                        <lightning:input type="number" class="myCustomClass"  disabled="true" value="{!v.ClaimsDetails[0].Total_Agreed_by_AG_LL__c}" name="currencyVal" formatter="currency" step="0.01" />
                                                        <!-- <input class="form-control" disabled="true" placeholder="{!'£'+v.ClaimsDetails[0].Total_Agreed_by_AG_LL__c}"/> -->
                                                    </div>
                                                </div>
                                                <p>Thank you for agreeing to the agent/landlord's claim. Please use the link below to arrange repayment.  </p>
                                            </div>
                                            <div class="text-right">
                                                <lightning:button class="see-all mw-auto" onclick="{!c.makePayment}" >Make Payment </lightning:button> 
                                            </div>
                                        </div>
                                    </section>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </aura:if> 
        <aura:if isTrue="{!v.ViewContinue}">
            <div class="tab-content custom-tab-content" id="sf-tabContent">
                
                <div class="tab-pane fade show active" id="sf-deposit" role="tabpanel" aria-labelledby="sf-deposit-tab">
                    
                    <div class="list-of-btns" id="scrollView">
                        <div class="container">
                            <lightning:button onclick="{!c.goToCliamSummary}" aura:id="div0" class="go-back"><img  src="{!$Resource.EI_ZD_Assets  + '/img/zd-arrow-dropleft.png'}" alt="Back Icon"/> Go Back</lightning:button>
                            <aura:if isTrue="{!v.isLead}">
                                <lightning:button variant="brand" label="I agree with the agent/landlord’s claim" title="I agree with the agent/landlord’s claim"
                                                  onclick="{!c.doAgreeToClaim }" class="see-all"/>
                            </aura:if>
                        </div>
                    </div>
                    
                    <div class="deposit-agent" >
                        <div class="container">
                            <div class="row">
                                <div class="col-md-9">
                                    <section class="deposit_steps">
                                        <div class="deposit_steps_detail">
                                            <h6>You have until <ui:outputDate value="{!v.ClaimsDetails[0].Respond_Date__c}" format="dd/MM/yyyy"></ui:outputDate> to try to reach a resolution. Please indicate how you wish to proceed below.</h6>
                                            <div class="claim-summary_gray-panel">
                                                <h5>Claim summary</h5>
                                                <div class="row">
                                                    <div class="col-lg-6 col-sm-9">
                                                        <p>Amount claimed by agent/landlord</p>
                                                    </div>
                                                    <div class="col-lg-6 col-sm-3">
                                                        <p><lightning:formattedNumber value="{!v.ClaimsDetails[0].Total_Agreed_by_AG_LL__c}" style="currency"  minimumFractionDigits="2" maximumFractionDigits="2"/></p>
                                                    </div>
                                                </div>
                                                <div class="row my-4">
                                                    <div class="col-lg-6 col-sm-9">
                                                        <p class="text-right">- of which is agreed by tenant</p>
                                                    </div>
                                                    <div class="col-lg-6 col-sm-3">
                                                        <p>(<lightning:formattedNumber value="{!v.ClaimsDetails[0].Total_Agreed_by_Tenant__c}" style="currency"  minimumFractionDigits="2" maximumFractionDigits="2"/>)</p>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-lg-6 col-sm-9">
                                                        <p><b>Amount outstanding</b></p>
                                                    </div>
                                                    <div class="col-lg-6 col-sm-3">
                                                        <p><b> <lightning:formattedNumber value="{!v.ClaimsDetails[0].Total_Agreed_by_AG_LL__c - v.ClaimsDetails[0].Total_Agreed_by_Tenant__c}" style="currency" minimumFractionDigits="2" maximumFractionDigits="2"/></b></p>
                                                    </div>
                                                </div>
                                            </div>
                                            
                                            <div class="row">
                                                <div class="col-lg-12 col-md-12">
                                                    <div class="agree-container">
                                                        <div class="agree-detail" style="display: block;">
                                                            <div class="form-group row">
                                                                <div class="col-sm-4">
                                                                    <div class="request-section">
                                                                        <label>Requested by you</label>
                                                                        <aura:iteration var="desptItems" items="{!v.ClaimsDetails[0].Dispute_Items__r}">
                                                                            <aura:if isTrue="{!desptItems.Type__c=='Other'}">
                                                                                <div class="form-group">
                                                                                    <label for="Others" class=" col-form-label">{!desptItems.Type__c}</label>
                                                                                    <div id="textarea"  contenteditable="true" disabled="true"  >
                                                                                        <div style="padding:2px; padding-right: 10px;">
                                                                                            <p id="ptag" disabled="true" > <lightning:formattedNumber value="{!desptItems.Agreed_by_AGLL__c}" style="currency"  minimumFractionDigits="2" maximumFractionDigits="2"/></p>
                                                                                            <p  id="ptag"  disabled="true" >{!desptItems.Other_Reason__c}</p>
                                                                                        </div>
                                                                                    </div>
                                                                                    <!-- <textarea rows="3" class="form-control" style="overflow: hidden;" disabled="true" id="Others">{!'£'+desptItems.Agreed_by_AGLL__c}&nbsp;
                                                                                    {!desptItems.Other_Reason__c}</textarea> -->
                                                                                </div>
                                                                                <aura:set attribute="else">
                                                                                    <div class="form-group">
                                                                                        <label for="Cleaning" class=" col-form-label">{!desptItems.Type__c}</label>
                                                                                        <lightning:input type="number" name="input1"  formatter="currency" step="0.01"  disabled="true"  value="{!desptItems.Agreed_by_AGLL__c}" />
                                                                                        <!-- <input type="number" class="form-control" disabled="true" id="Cleaning" placeholder="{!'£'+desptItems.Agreed_by_AGLL__c}"/> -->
                                                                                    </div>
                                                                                </aura:set>
                                                                            </aura:if>
                                                                        </aura:iteration>
                                                                    </div>
                                                                </div>
                                                                <div class="col-sm-4">
                                                                    <div class="response-section">
                                                                        <label>Previously agreed by tenant</label>
                                                                        <aura:iteration var="desptItems" items="{!v.ClaimsDetails[0].Dispute_Items__r}">
                                                                            <div class="form-group">
                                                                                <label class="col-form-label">{!desptItems.Type__c}</label>
                                                                                <lightning:input type="number" name="input1"  formatter="currency" step="0.01"  disabled="true"  value="{!desptItems.Agreed_by_Tenant__c}" />
                                                                                <!-- <input type="number" disabled="true" class="form-control" placeholder="{!'£'+desptItems.Agreed_by_Tenant__c}"/> -->
                                                                            </div>
                                                                        </aura:iteration>
                                                                    </div>
                                                                </div>
                                                                <div class="col-sm-4" style="pointer-events: none;">
                                                                    <div class="dispute-section">
                                                                        <label>Amount still outstanding</label>
                                                                        <aura:iteration var="desptItems" items="{!v.ClaimsDetails[0].Dispute_Items__r}">
                                                                            <div class="form-group">
                                                                                <label class="col-form-label">{!desptItems.Type__c}</label>
                                                                                <lightning:input type="number" name="input1"  formatter="currency" step="0.01"   value="{!sub(desptItems.Agreed_by_AGLL__c,desptItems.Agreed_by_Tenant__c)}" />
                                                                                <!-- <input type="text" disabled="true" class="form-control" placeholder="{!'£'+sub(desptItems.Agreed_by_AGLL__c,desptItems.Agreed_by_Tenant__c)}"/> -->
                                                                            </div>
                                                                        </aura:iteration>
                                                                    </div>
                                                                </div>
                                                                <!-- 
                                                            <aura:iteration var="desptItems" items="{!v.ClaimsDetails[0].Dispute_Items__r}">
                                                                <div class="col-sm-4">
                                                                    <div class="request-section">
                                                                        <aura:if isTrue="{!desptItems.Type__c=='Other'}">
                                                                            <div class="form-group">
                                                                                <label for="Others" class=" col-form-label">{!desptItems.Type__c}</label>
                                                                                <div id="textarea"  contenteditable="true" disabled="true"  >
                                                                                    <div style="padding:2px; padding-right: 10px;">
                                                                                        <p id="ptag" disabled="true" > <lightning:formattedNumber value="{!desptItems.Agreed_by_AGLL__c}" style="currency"  minimumFractionDigits="2" maximumFractionDigits="2"/></p>
                                                                                        <p  id="ptag"  disabled="true" >{!desptItems.Other_Reason__c}</p>
                                                                                    </div>
                                                                                </div>
                                                                               
                                                                            </div>
                                                                            <aura:set attribute="else">
                                                                                <div class="form-group">
                                                                                    <label for="Cleaning" class=" col-form-label">{!desptItems.Type__c}</label>
                                                                                    <lightning:input type="number" name="input1"  formatter="currency" step="0.01"  disabled="true"  value="{!desptItems.Agreed_by_AGLL__c}" />
                                                                                   </div>
                                                                            </aura:set>
                                                                        </aura:if>
                                                                    </div>
                                                                </div>
                                                                <div class="col-sm-4">
                                                                    <div class="response-section">
                                                                        <div class="form-group">
                                                                            <label class="col-form-label">{!desptItems.Type__c}</label>
                                                                            <lightning:input type="number" name="input1"  formatter="currency" step="0.01"  disabled="true"  value="{!desptItems.Agreed_by_Tenant__c}" />
                                                                           </div>
                                                                    </div>
                                                                </div>
                                                                <div class="col-sm-4">
                                                                    <div class="dispute-section">
                                                                        <div class="form-group">
                                                                            <label class="col-form-label">{!desptItems.Type__c}</label>
                                                                            <lightning:input type="number" name="input1"  formatter="currency" step="0.01"  disabled="true"  value="{!sub(desptItems.Agreed_by_AGLL__c,desptItems.Agreed_by_Tenant__c)}" />
                                                                            </div>
                                                                    </div>
                                                                </div>
                                                            </aura:iteration> -->
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div> 
                                            
                                            <aura:if isTrue="{!v.ClaimsDetails[0].Claim_exceeds_the_total_level_of_cover__c}">
                                                <div class="form-group row">
                                                    <label class="col-sm-4 col-form-label">Total claim amount</label>
                                                    <div class="col-sm-8">
                                                        <p>The agent/landlord has indicated that their total claim exceeds the deposit amount and have given the following breakdown. You can respond to individual claim areas in the next page(s). The maximum the adjudicator can award is the deposit amount.</p>
                                                        
                                                        <div class="form-group">
                                                            <lightning:textarea disabled="true" class=" topText" value="{!v.ClaimsDetails[0].Claim_exceeds_comment_AGLL__c}"  name="input1"  />
                                                        </div>
                                                    </div>
                                                </div>
                                            </aura:if>
                                            
                                            
                                            <div class="text-right">
                                                <lightning:button variant="brand" label="Continue" onclick="{!c.goToClaimBreakdown}" class="see-all"/>
                                            </div>
                                        </div>
                                    </section>
                                    
                                </div>
                                <div class="col-md-3 info-right-tab">
                                    <p>This is your opportunity to provide details of your claim and evidence to support it. You can return to edit your response at any time up to <strong><ui:outputDate value="{!v.ClaimsDetails[0].Respond_Date__c}" format="dd/MM/yyyy"></ui:outputDate></strong>. During the process, your submission will be saved automatically each time you press ‘Continue’.</p>
                                    
                                    <p>After <strong><ui:outputDate value="{!v.ClaimsDetails[0].Respond_Date__c}" format="dd/MM/yyyy"></ui:outputDate></strong>, your information will be made available to the tenant and they will be asked to provide their response. The adjudicator will use all the information from both parties in making their decision. It is important that you provide the information you wish the adjudicator to see now. SafeDeposits will generally not contact you again to ask for more information.</p>
                                    
                                    <p>Please respond fully to all the questions asked. It is your responsibility to explain your claim clearly and highlight the relevant evidence.</p>
                                    
                                    <p>You will be asked first whether you have some key documents. Please be aware that the success of your claim may be compromised if you are unable to provide them. If you do not have one or more of the key documents, please consider whether you wish to continue with your claim. You can opt to withdraw from the dispute at any point during the evidence submission process by selecting the option to ‘Cancel dispute’. The outstanding balance of the deposit would be returned to the tenant.</p>
                                    
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </aura:if>
        <aura:if isTrue="{!v.showClaimBreakdown}">
            <div class="tab-content custom-tab-content" id="sf-tabContent">
                
                <div class="tab-pane fade show active" id="sf-deposit" role="tabpanel" aria-labelledby="sf-deposit-tab">
                    <div class="list-of-btns">
                        <div class="container">
                            <lightning:button onclick="{!c.goToCliamSummary}" aura:id="div2" class="go-back"><img  src="{!$Resource.EI_ZD_Assets  + '/img/zd-arrow-dropleft.png'}" alt="Back Icon"/> Go Back</lightning:button>
                            <aura:if isTrue="{!v.isLead}">
                                <lightning:button variant="brand" label="I agree with the agent/landlord’s claim" title="I agree with the agent/landlord’s claim"
                                                  onclick="{!c.doAgreeToClaim }" class="see-all"/>
                            </aura:if>
                        </div>
                        
                    </div>
                    <div class="deposit-agent" >
                        
                        <div class="container" id="scrollView">
                            <div class="row">
                                <div class="col-md-9">
                                    
                                    <section class="deposit_steps">
                                        
                                        <div class="deposit_steps_count">
                                            <div class="state-indicator">
                                                <span class="circle">1</span>
                                                <p>Key documents</p>
                                            </div>
                                            <div class="state-indicator active">
                                                <span class="circle">2</span>
                                                <p>Claim breakdown</p>
                                            </div>
                                            <div class="state-indicator">
                                                <span class="circle">3</span>
                                                <p>Additional comments</p>
                                            </div>
                                            <div class="state-indicator">
                                                <span class="circle">4</span>
                                                <p>Review submission</p>
                                            </div>
                                        </div>
                                        
                                        <div class="deposit_steps_detail" >
                                            
                                            <form>
                                                <aura:iteration var="desptItems" items="{!v.ClaimsDetails[0].Dispute_Items__r}" indexVar="i">
                                                    <aura:if isTrue="{!v.currentItem==i}">
                                                        
                                                        <aura:if isTrue="{!desptItems.Type__c=='Cleaning'}">
                                                            <aura:if isTrue="{!v.agentResponse}">
                                                                
                                                                <h6 class="blue-clr" id="scrollView" >Your claim for <span class="lowercase">{!desptItems.Type__c}</span> </h6>
                                                                <div class="row">
                                                                    <div class="col-lg-12 col-md-12">
                                                                        <div class="agree-container">
                                                                            <div class="agree-detail" style="display: block;">
                                                                                <div class="form-group row">
                                                                                    <div class="col-sm-4">
                                                                                        <div class="request-section">
                                                                                            <label>Requested by you</label>
                                                                                            <div class="form-group">
                                                                                                <lightning:input type="number" name="input1"  formatter="currency" step="0.01"  disabled="true"  value="{!desptItems.Agreed_by_AGLL__c}" />
                                                                                                <!--<input type="number" disabled="true" class="form-control" id="Cleaning" placeholder="{!'£'+desptItems.Agreed_by_AGLL__c}" /> -->
                                                                                            </div>
                                                                                        </div>
                                                                                    </div>
                                                                                    <div class="col-sm-4">
                                                                                        <div class="response-section">
                                                                                            <label>Previously agreed by tenant</label>
                                                                                            <div class="form-group">
                                                                                                <lightning:input type="number" name="input1"  formatter="currency" step="0.01"  disabled="true"  value="{!desptItems.Agreed_by_Tenant__c}" />
                                                                                                <!-- <input type="number" disabled="true" class="form-control" placeholder="{!'£'+desptItems.Agreed_by_Tenant__c}" /> -->
                                                                                            </div>
                                                                                        </div>
                                                                                    </div>
                                                                                    <div class="col-sm-4" style="pointer-events: none;">
                                                                                        <div class="dispute-section">
                                                                                            <label>Amount still outstanding</label>
                                                                                            <div class="form-group">
                                                                                                <lightning:input type="number" name="input1"  formatter="currency" step="0.01"   value="{!sub(desptItems.Agreed_by_AGLL__c,desptItems.Agreed_by_Tenant__c)}" />
                                                                                                <!-- <input type="text" disabled="true" class="form-control" placeholder="{!'£'+sub(desptItems.Agreed_by_AGLL__c,desptItems.Agreed_by_Tenant__c)}" /> -->
                                                                                            </div>
                                                                                        </div>
                                                                                    </div>
                                                                                </div>
                                                                                
                                                                                
                                                                                <div class="form-group row">
                                                                                    <label class="col-sm-4 col-form-label">Please describe your claim for cleaning</label>
                                                                                    <div class="col-sm-8">
                                                                                        <lightning:textarea  rows="4" aura:id="Cleaning" disabled="true"   value="{!desptItems.Claim_description_for_cleaning_agll__c}" placeholder=""></lightning:textarea>
                                                                                    </div>
