<aura:component implements="force:appHostable,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction" access="global"
                controller="EI_AGLLevidencegathering">
    <aura:attribute name="claimId" type="string" default="" />
    <aura:attribute name="viewClaim" type="Boolean" default="true" />
    <aura:attribute name="showCancelDiv" type="Boolean" default="false" />
    <aura:attribute name="ViewContinue" type="Boolean" default="false" />
    <aura:attribute name="keyDocuments" type="Boolean" default="false" />
    <aura:attribute name="consentBoxValue" type="Boolean" default="false" />
    <aura:attribute name="showConsentError" type="Boolean" default="false" />
    <aura:attribute name="showClaimBreakdown" type="Boolean" default="false" />
    <aura:attribute name="showKeyDocumentsError" type="Boolean" default="false" />
    <aura:attribute name="documentsskipError" type="Boolean" default="false" />
    <aura:attribute name="showAdditionalComments" type="Boolean" default="false" />
    <aura:attribute name="showReviewsubmission" type="Boolean" default="false" />
    <aura:attribute name="isClaimForRent" type="Boolean" default="false" />
    <aura:attribute name="showDisputeItemError" type="Boolean" default="false" />
    <aura:attribute name="consent"  type="Boolean" default="false"/>
    <aura:attribute name="Isclaimamountexceed"  type="Boolean" default="false"/>
    
    <aura:attribute name="currentClaimCatagories" type="string" default="" />
    <aura:attribute name="currentItem" type="Integer" default="0" />
    <aura:attribute name="ClaimsDetails" type="list" />
    
    <aura:attribute name="depositid" type="string" />
    
    <aura:attribute name="isEditable" type="boolean" default="false" />
    
    <!--Error Message Attribute-->
    	<aura:attribute name="eviGatheringErrorNew" type="String" />
        <aura:attribute name="evigatAGLLErrorNew" type="String" />
        <aura:attribute name="evigatDocErrorNew" type="String"/>
    	<aura:attribute name="evidenceErrorNew" type="String"/>
    	<aura:attribute name="errorList" type="Error_Message__mdt[]"/>
    
    <lightning:navigation aura:id="navService"/>
    
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <!-- <aura:handler name="EI_ZD_refreshParentView" event="c:EI_ZD_refreshParentView" action="{!c.refreshParentViewEvent}"/> -->
 <!--   <aura:registerEvent name="EI_ZD_refreshParentView" type="c:EI_ZD_refreshParentView" />-->
    <body >
        <div class="container">
            <div class="row">
                <div class="col-md-9" style="margin-bottom:-30px">
                     <aura:if isTrue="{!v.ClaimsDetails[0].AGLL_Respond_Evidance_Gathering__c==true}">
                         <h2>Review/edit evidence</h2>
                         <aura:set attribute="else">
                            <h2>Submit evidence</h2>
                         </aura:set>
                      </aura:if> 
                </div>             
            </div>
        </div>  
        
       <!-- <div class="col-md-12" >      
      			<h2 style="margin-bottom:5px">Review /edit evidence</h2>
    		</div>-->         
        <aura:if isTrue="{!v.ViewContinue}">
            <div class="tab-content custom-tab-content" id="sf-tabContent">
                
                <div class="tab-pane fade show active" id="sf-deposit" role="tabpanel" aria-labelledby="sf-deposit-tab">
                    
                    <div class="list-of-btns">
                        <div class="container">
                            <lightning:button onclick="{!c.goTodepositsummary}" aura:id="div0" class="go-back"><img  src="{!$Resource.TDSTheme  + '/assets/img/md-arrow-dropleft.png'}" alt="Back Icon"/> Go back</lightning:button>
                            <lightning:button variant="brand" label="Cancel dispute " title="Continue claim journey"
                                              onclick="{! c.cancelClaim }" class="see-all"/>
                        </div>
                    </div>
                    
                    <div class="deposit-agent">
                        <div class="container">
                            <div class="row">
                                <div class="col-md-9">
                                    <section class="deposit_steps">
                                        <div class="deposit_steps_detail">
                                          <!--  <h6>You have until <lightning:formattedDateTime value="{!v.ClaimsDetails[0].Respond_Date__c}" year="numeric" month="numeric" day="numeric" /> to submit details of your case and your evidence.</h6>-->
                                            <h6>You have until <ui:outputDate format="DD/MM/YYYY" value="{!v.ClaimsDetails[0].Respond_Date__c}" /> to submit details of your case and your evidence.</h6>
                                            <!--Paid amount start-->
                                              <div class="claim-summary_gray-panel">
                                               <strong><p>We have released the following amounts in accordance with the parties' instructions</p></strong>
                                                  <br/>
                                                <div class="row">
                                                    <div class="col-lg-6 col-sm-9">
                                                        <p>Amount repaid to agent/landlord</p>
                                                    </div>
                                                    <div class="col-lg-6 col-sm-3">
                                                            <p><lightning:formattedNumber value="{!v.ClaimsDetails[0].Total_Agreed_by_Tenant__c}" style="currency"  minimumFractionDigits="2" maximumFractionDigits="2"/></p>
                                                    </div>
                                                </div>
                                                <div class="row my-4">
                                                    <div class="col-lg-6 col-sm-9">
                                                        <p >Amount repaid to tenant(s)</p>
                                                    </div>
                                                    <div class="col-lg-6 col-sm-3">
                                                        <p><lightning:formattedNumber value="{!v.ClaimsDetails[0].Agreed_amount_from_AGLL_to_TT__c}" style="currency"  minimumFractionDigits="2" maximumFractionDigits="2"/></p>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-lg-6 col-sm-9">
                                                        <p><b>Total amount repaid</b></p>
                                                    </div>
                                                    <div class="col-lg-6 col-sm-3">
                                                        <p><b> <lightning:formattedNumber value="{!v.ClaimsDetails[0].Agreed_amount_from_AGLL_to_TT__c + v.ClaimsDetails[0].Total_Agreed_by_Tenant__c}" style="currency" minimumFractionDigits="2" maximumFractionDigits="2"/></b></p>
                                                    </div>
                                                </div>
                                            </div>
                                            <!--Paid amount end-->
                                            
                                            <div class="claim-summary_gray-panel">
                                                <h5>Dispute summary</h5>
                                                <div class="row">
                                                    <div class="col-lg-6 col-sm-9">
                                                        <p>Amount requested by agent/landlord</p>
                                                    </div>
                                                    <div class="col-lg-6 col-sm-3">
                                                        <p><lightning:formattedNumber value="{!v.ClaimsDetails[0].Total_Agreed_by_AG_LL__c}" style="currency"  minimumFractionDigits="2" maximumFractionDigits="2"/></p>
                                                    </div>
                                                </div>
                                                <div class="row my-4">
                                                    <div class="col-lg-6 col-sm-9">
                                                        <p class="text-right">- Of which is agreed by tenant</p>
                                                    </div>
                                                    <div class="col-lg-6 col-sm-3">
                                                        <p>(<lightning:formattedNumber value="{!v.ClaimsDetails[0].Total_Agreed_by_Tenant__c}" style="currency"  minimumFractionDigits="2" maximumFractionDigits="2"/>)</p>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-lg-6 col-sm-9">
                                                        <p><b>Disputed amount held by SafeDeposits</b></p>
                                                    </div>
                                                    <div class="col-lg-6 col-sm-3">
                                                        <p><b> <lightning:formattedNumber value="{!v.ClaimsDetails[0].Total_Agreed_by_AG_LL__c - v.ClaimsDetails[0].Total_Agreed_by_Tenant__c}" style="currency" minimumFractionDigits="2" maximumFractionDigits="2"/></b></p>
                                                    </div>
                                                </div>
                                            </div>
                                            
                                            <div class="row">
                                                <div class="col-lg-12 col-md-12">
                                                    <div class="agree-container">
                                                        <div class="agree-detail" style="display: block;">
                                                            <div class="form-group row">
                                                                <div class="col-sm-4">
                                                                    <div class="request-section">
                                                                        <label>Requested by you</label>
                                                                    </div>
                                                                </div>
                                                                <div class="col-sm-4">
                                                                    <div class="response-section">
                                                                        <label>Previously agreed by tenant</label>
                                                                    </div>
                                                                </div>
                                                                <div class="col-sm-4">
                                                                    <div class="dispute-section">
                                                                        <label>Amount in dispute</label>
                                                                    </div>
                                                                </div>
                                                                
                                                                <aura:iteration var="desptItems" items="{!v.ClaimsDetails[0].Dispute_Items__r}">
                                                                    <div class="col-sm-4">
                                                                        <div class="request-section">
                                                                            <aura:if isTrue="{!desptItems.Type__c=='Other'}">
                                                                                <div class="form-group">
                                                                                    <label for="Others" class=" col-form-label">{!desptItems.Type__c}</label>
                                                                                    <div id="textarea"  contenteditable="true" disabled="true"  >
                                                                                        <div style="padding:2px; padding-right: 10px;">
                                                                                            <p id="ptag" disabled="true" > <lightning:formattedNumber value="{!desptItems.Agreed_by_AGLL__c}" style="currency"  minimumFractionDigits="2" maximumFractionDigits="2"/></p>
                                                                                            <p  id="ptag"  disabled="true" >{!desptItems.Other_Reason__c}</p>
                                                                                        </div>
                                                                                    </div>
                                                                                </div>
                                                                                <aura:set attribute="else">
                                                                                    <div class="form-group">
                                                                                        <label for="Cleaning" >{!desptItems.Type__c}</label>
                                                                                       <!-- <input type="number" class="form-control" disabled="true" id="Cleaning" placeholder="{!'£'+desptItems.Agreed_by_AGLL__c}"/> -->
                                                                                       <lightning:input type="number" name="input1"  formatter="currency" step="0.01"  disabled="true"  value="{!desptItems.Agreed_by_AGLL__c}" />
                                                                                    
                                                                                    </div>
                                                                                </aura:set>
                                                                            </aura:if>
                                                                        </div>
                                                                    </div>
                                                                    <div class="col-sm-4">
                                                                        <div class="response-section">
                                                                            <div class="form-group">
                                                                                <label >{!desptItems.Type__c}</label>
                                                                                <lightning:input type="number" name="input1"  formatter="currency" step="0.01"  disabled="true"  value="{!desptItems.Agreed_by_Tenant__c}" />
                                                                                <!--<input type="number" disabled="true" class="form-control" placeholder="{!'£'+desptItems.Agreed_by_Tenant__c}"/> -->
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                    <div class="col-sm-4">
                                                                        <div class="dispute-section">
                                                                            <div class="form-group">
                                                                                <label >{!desptItems.Type__c}</label>
                                                                                <lightning:input type="number" name="input1"  formatter="currency" step="0.01"  disabled="true"  value="{!sub(desptItems.Agreed_by_AGLL__c,desptItems.Agreed_by_Tenant__c)}" />
                                                                               <!-- <input type="text" disabled="true" class="form-control" placeholder="{!'£'+sub(desptItems.Agreed_by_AGLL__c,desptItems.Agreed_by_Tenant__c)}"/> -->
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </aura:iteration>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <aura:if isTrue ="{!v.isEditable}">   
                                                <h6>Please give your consent to the following:</h6>
                                                
                                                <p>I give authority to SafeDeposits Scotland to consider my dispute and confirm that:</p>
                                                
                                                <ul class="custom-list">
                                                    <li>I accept responsibility for ensuring that any personal data contained in the information I submit complies with the Data Protection Act 1988 and the General Data Protection Regulation;</li>
                                                    <li>I consent to the information I provide being shared with the other party to the dispute, the adjudicator and anyone else involved in resolving it;</li>
                                                    <li>I agree that the decision of the adjudicator will be final and binding, subject to the review process .</li>
                                                </ul>
                                                
                                                <div class="form-row">
                                                    <div class="form-group col-md-12 text-right">
                                                        <div class="form-check">
                                                            <input type="checkbox" checked="{!v.consent}" class="form-check-input" id="exampleCheck2" />
                                                            <label class="form-check-label"  for="exampleCheck2">I consent to the above</label>
                                                            <aura:if isTrue="{!v.showConsentError}">
                                                                <p class="error-msg text-right">
                                                                    <!--You must give your authority for the dispute to be considered. -->
                                                                    {!v.eviGatheringErrorNew}
                                                                </p>
                                                            </aura:if>
                                                        </div>
                                                        
                                                    </div>
                                                </div>
                                            </aura:if>
                                            <div class="text-right">
                                                <lightning:button variant="brand" label="Continue" onclick="{!c.goToKeyDocuments}" class="see-all"/>
                                            </div>
                                        </div>
                                    </section>
                                    
                                </div>
                                <div class="col-md-3 info-right-tab">
                                    <p>This is your opportunity to provide details of your claim and evidence to support it. You can return to edit your response at any time up to <strong><ui:outputDate format="DD/MM/YYYY" value="{!v.ClaimsDetails[0].Respond_Date__c}" /></strong>. During the process, your submission will be saved automatically each time you press ‘Continue’.</p>
                                    
                                    <p>After <strong><ui:outputDate format="DD/MM/YYYY" value="{!v.ClaimsDetails[0].Respond_Date__c}" /></strong>, your information will be made available to the tenant and they will be asked to provide their response. The adjudicator will use all the information from both parties in making their decision. It is important that you provide the information you wish the adjudicator to see now. SafeDeposits will generally not contact you again to ask for more information.</p>
                                    
                                    <p>Please respond fully to all the questions asked. It is your responsibility to explain your claim clearly and highlight the relevant evidence.</p>
                                    
                                    <p>You will be asked first whether you have some key documents. Please be aware that the success of your claim may be compromised if you are unable to provide them. If you do not have one or more of the key documents, please consider whether you wish to continue with your claim. You can opt to withdraw from the dispute at any point during the evidence submission process by selecting the option to ‘Cancel dispute’. The outstanding balance of the deposit would be returned to the tenant.</p>
                                    
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </aura:if>
        <aura:if isTrue="{!v.keyDocuments}">
            <div class="tab-content custom-tab-content" id="sf-tabContent">
                
                <div class="tab-pane fade show active" id="sf-deposit" role="tabpanel" aria-labelledby="sf-deposit-tab">
                    
                    <div class="list-of-btns">
                        <div class="container">
                            <lightning:button onclick="{!c.goToCliamSummary}" aura:id="div1" class="go-back"><img  src="{!$Resource.TDSTheme  + '/assets/img/md-arrow-dropleft.png'}" alt="Back Icon"/> Go Back</lightning:button>
                            <lightning:button variant="brand" label="Cancel dispute " title="Continue claim journey"
                                              onclick="{! c.cancelClaim }" class="see-all"/>
                        </div>
                    </div>
                    
                    <div class="deposit-agent">
                        <div class="container" id="scrollView">
                            <div class="row">
                                <div class="col-md-9">
                                    
                                    <section class="deposit_steps">
                                        
                                        <div class="deposit_steps_count">
                                            <div class="state-indicator active">
                                                <span class="circle">1</span>
                                                <p>Key documents</p>
                                            </div>
                                            <div class="state-indicator">
                                                <span class="circle">2</span>
                                                <p>Claim breakdown</p>
                                            </div>
                                            <div class="state-indicator">
                                                <span class="circle">3</span>
                                                <p>Additional comments</p>
                                            </div>
                                            <div class="state-indicator">
                                                <span class="circle">4</span>
                                                <p>Review submission</p>
                                            </div>
                                        </div>
                                        
                                        <div class="deposit_steps_detail" >
                                            <form>
                                                <aura:if isTrue = "{!v.isEditable}">
                                                   
                                                    <aura:if isTrue = "{!v.Isclaimamountexceed}"> 
                                                     <div class="form-group row">              
                                                        <label class="col-sm-4 col-form-label">Total claim amount</label>
                                                        <div class="col-sm-8">
                                                            <label class="col-form-label evi_label">Does your total claim exceed the deposit amount?</label>
                                                            <aura:if isTrue="{!v.ClaimsDetails[0].Claim_exceed__c =='Yes'}">
                                                                <a class="learn-more active" id="Exceedclaim" onclick="{!c.clickYes}">Yes</a>
                                                                <a class="learn-more" id="Exceedclaim" onclick="{!c.clickNo}" >No</a>
                                                                <aura:set attribute="else">
                                                                    <aura:if isTrue="{!v.ClaimsDetails[0].Claim_exceed__c =='No'}">
                                                                        <a class="learn-more" id="Exceedclaim" onclick="{!c.clickYes}">Yes</a>
                                                                        <a class="learn-more active"  id="Exceedclaim" onclick="{!c.clickNo}" >No</a>
                                                                        <aura:set attribute="else">
                                                                            <a class="learn-more" id="Exceedclaim" onclick="{!c.clickYes}">Yes</a>
                                                                            <a class="learn-more" id="Exceedclaim" onclick="{!c.clickNo}" >No</a>
                                                                        </aura:set>
                                                                    </aura:if>
                                                                </aura:set>
                                                            </aura:if>
                                                            
                                                            
                                                        </div>
                                                    </div>
                                                    </aura:if>
                                                    
                                                 <!--  <aura:if isTrue="{!v.ClaimsDetails[0].Claim_exceeds_the_total_level_of_cover__c}">-->
                                                     <aura:if isTrue="{!v.ClaimsDetails[0].Claim_exceed__c =='Yes'}">
                                                    <div class="form-group row">
                                                        <div class="col-lg-12 col-md-12">
                                                            <p>If it is the case that your total claim exceeds the deposit, you have the option to tell the adjudicator about this. Please provide a full breakdown of the claim below. The adjudicator will consider this information when making their award, but can only award a maximum of the deposit. Please provide a breakdown of your claim below.</p>
                                                            <div class="form-group">
                                                                <lightning:textarea disabled="{!!v.isEditable}"  class="topText" value="{!v.ClaimsDetails[0].Claim_exceeds_comment_AGLL__c	}"  name="input1"  />
                                                                
                                                            </div>
                                                        </div>
                                                    </div>
                                                    </aura:if>
                                                    
                                              <!--  </aura:if>  -->                                                  
                                                    
                                                    
                                                    <div class="form-group row">
                                                        <label class="col-sm-4 col-form-label">Tenant's obligations and use of the deposit</label>
                                                        <div class="col-sm-8">
                                                            <label class="col-form-label evi_label">Do you have a tenancy agreement?</label>
                                                            <aura:if isTrue="{!v.ClaimsDetails[0].Tenant_obligations__c =='Yes'}">
                                                                <a class="learn-more active" id="TenantObligations" onclick="{!c.clickYes}">Yes</a>
                                                                <a class="learn-more" id="TenantObligations" onclick="{!c.clickNo}" >No</a>
                                                                <aura:set attribute="else">
                                                                    <aura:if isTrue="{!v.ClaimsDetails[0].Tenant_obligations__c =='No'}">
                                                                        <a class="learn-more" id="TenantObligations" onclick="{!c.clickYes}">Yes</a>
                                                                        <a class="learn-more active"  id="TenantObligations" onclick="{!c.clickNo}" >No</a>
                                                                        <aura:set attribute="else">
                                                                            <a class="learn-more" id="TenantObligations" onclick="{!c.clickYes}">Yes</a>
                                                                            <a class="learn-more" id="TenantObligations" onclick="{!c.clickNo}" >No</a>
                                                                        </aura:set>
                                                                    </aura:if>
                                                                </aura:set>
                                                            </aura:if>
                                                            
                                                            <aura:if isTrue="{!v.ClaimsDetails[0].Tenant_obligations__c =='No'}">
                                                                <aura:if isTrue ="{!v.isEditable}"> 
                                                                    <div class="row">
                                                                        <div class="col-lg-9">
                                                                            <div class="alert alert-warning alert-dismissible fade show">
                                                                                <img src="{!$Resource.TDSTheme  + '/assets/img/warning-icon.png'}" alt="warning Icon"/>
                                                                                <div>
                                                                                    <strong>Warning!</strong> Without this document, your chances of success may be compromised. You can opt to withdraw from the dispute at any time during the evidence gathering process by selecting 'Cancel dispute'. This means that the outstanding deposit would be returned to the tenant(s).
                                                                                </div>
                                                                                <button type="button" class="close" data-dismiss="alert">×</button>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </aura:if>
                                                            </aura:if>
                                                            
                                                        </div>
                                                    </div>
                                                   
                                                    <aura:if isTrue="{!v.ClaimsDetails[0].Tenant_obligations__c =='Yes'}">
                                                  <!-- <c:EI_EvidenceUploadcomp isEditable="{!v.isEditable}" recordId="{!v.ClaimsDetails[0].Id}" evidanceAttachment="{!v.ClaimsDetails[0].Evidence_Attachments__r}" evidenceCategories="Tenant obligations" userType="{!v.ClaimsDetails[0].Dispute_Initiated_By__c}" source="Dispute Response" scheme="Zero Deposite" />-->
                                                   <c:EI_EvidenceUploadcomp isEditable="{!v.isEditable}" recordId="{!v.ClaimsDetails[0].Id}" evidanceAttachment="{!v.ClaimsDetails[0].Evidence_Attachments__r}" evidenceCategories="Tenant obligations" userType="{!v.ClaimsDetails[0].Dispute_Initiated_By__c}" source="Dispute Response" scheme="SDS" />     
                                                        <!--
                                                    <div class="form-group row">
                                                        <div class="col-lg-12 col-md-12">
                                                            <div class="file_section">
                                                                <div class="row">
                                                                    <div class="col-md-5 col-sm-12">
                                                                        <lightning:input aura:id="fileId" onchange="{!c.handleUploadFinished}" type="file" name="file" label="Upload Attachment" multiple="false"/>
                                                                    </div>
                                                                    <div class="col-md-7 col-sm-12">
                                                                        <div class="select_file_section">
                                                                            <div class="no_file">
                                                                                <span></span>
                                                                                <p>No file Selected</p>
                                                                            </div>
                                                                            <div class="upload_range_part">
                                                                                <div>
                                                                                    <span class="upload_range"></span>
                                                                                    <p class="text-right">0kb of 0kb</p>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
             -->
                                                    </aura:if>
                                                    
                                                    
                                                    
                                                    
                                                    <div class="form-group row">
                                                        <label class="col-sm-4 col-form-label">Condition of the property at the start of the tenancy</label>
                                                        <div class="col-sm-8">
                                                            <label class="col-form-label evi_label">Do you have an inventory/check-in report?</label>
                                                            <aura:if isTrue="{!v.ClaimsDetails[0].inventorycheck_in_report_AGLL__c =='Yes'}">
                                                                <a class="learn-more active" id="inventorycheck" onclick="{!c.clickYes}">Yes</a>
                                                                <a class="learn-more" id="inventorycheck" onclick="{!c.clickNo}">No</a>
                                                                <aura:set attribute="else">
                                                                    <aura:if isTrue="{!v.ClaimsDetails[0].inventorycheck_in_report_AGLL__c =='No'}">
                                                                        <a class="learn-more" id="inventorycheck" onclick="{!c.clickYes}">Yes</a>
                                                                        <a class="learn-more active" id="inventorycheck" onclick="{!c.clickNo}">No</a>
                                                                        <aura:set attribute="else">
                                                                            <a class="learn-more" id="inventorycheck" onclick="{!c.clickYes}">Yes</a>
                                                                            <a class="learn-more" id="inventorycheck" onclick="{!c.clickNo}">No</a>
                                                                        </aura:set>
                                                                    </aura:if>
                                                                </aura:set>
                                                            </aura:if>
                                                            
                                                            <aura:if isTrue ="{!v.isEditable}"> 
                                                                <aura:if isTrue="{!v.ClaimsDetails[0].inventorycheck_in_report_AGLL__c =='No'}">
                                                                    <div class="row">
                                                                        <div class="col-lg-9">
                                                                            <div class="alert alert-warning alert-dismissible fade show">
                                                                                <img src="{!$Resource.TDSTheme  + '/assets/img/warning-icon.png'}" alt="warning Icon"/>
                                                                                <div>
                                                                                    <strong>Warning!</strong> Without this document, your chances of success may be compromised. You can opt to withdraw from the dispute at any time during the evidence gathering process by selecting 'Cancel dispute'. This means that the outstanding deposit would be returned to the tenant(s).
                                                                                </div>
                                                                                <button type="button" class="close" data-dismiss="alert">×</button>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </aura:if>
                                                            </aura:if>
