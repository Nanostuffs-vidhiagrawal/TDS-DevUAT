                <button
                  type="button"
                  class="close"
                  onclick="{!c.hideBootstrapErrors}"
                  name="lastnamemorechange"
                  >&times;</button
                >
              </div>
            </aura:if>
                  
               <aura:if isTrue="{!v.addressError}">
