<aura:component implements="force:appHostable,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction" access="global"
                controller="EI_Tenantevidencegatheringclass">
    <aura:attribute name="ClaimsDetails" type="list" />
    <aura:attribute name="viewClaim" type="Boolean" default="true" />
    <aura:attribute name="isLead" type="Boolean" default="false"/>
    <aura:attribute name="showConfirmDiv" type="boolean" default="false"/>
    <aura:attribute name="ViewContinue" type="boolean" default="false"/>
    <aura:attribute name="showConsentError" type="boolean" default="false"/>
    <aura:attribute name="showClaimBreakdown" type="boolean" default="false"/>
    <aura:attribute name="currentItem" type="Integer" default="0" />
    <aura:attribute name="agentResponse" type="boolean" default="true" />
    <aura:attribute name="showAdditionalComments" type="boolean" default="false" />
    <aura:attribute name="showReviewsubmission" type="boolean" default="false" />
    <aura:attribute name="currentClaimCatagories" type="string" default="" />
    <aura:attribute name="showDisputeItemError" type="boolean" default="false" />
    <aura:attribute name="isEditable" type="boolean" default="true" />
    <aura:attribute name="LeadTenantUrl" type="string"  default=""/>
    <aura:attribute name="depositid" type="string"/>
    <aura:attribute name="consent" type="boolean"/>
    <aura:attribute name="noCosent" type="boolean"/>
    <aura:attribute name="stepsSequence" type="Map"/>
     <aura:attribute name="PageSpinner" type="boolean" default="true"/> 
       <aura:handler event="aura:doneWaiting" action="{!c.doneWaiting}"></aura:handler>
    <!--Error message attribute-->
    	<aura:attribute name="tenantgathErrorNew" type="String" />
        <aura:attribute name="tenantRespondErrorNew" type="String" />
        <aura:attribute name="tenantFieldErrorNew" type="String" />       
    	<aura:attribute name="errorList" type="Error_Message__mdt[]"/>
    
    
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <aura:handler name="change" value="{!v.showClaimBreakdown}" action="{!c.claimBreakdown}"/>
    <lightning:navigation aura:id="navService"/>
          <aura:renderIf isTrue="{!v.PageSpinner}">
      <div class="lds-dual-ring">
      </div>
      <aura:set attribute="else">   
    <aura:if isTrue="{!v.viewClaim}"> -----------------view claim sectinon------------------
        <div class="tab-content custom-tab-content" id="sf-tabContent">
            <div class="tab-pane fade show active" id="sf-deposit" role="tabpanel" aria-labelledby="sf-deposit-tab">
                <!-- Resolution Section HTML Start -->
                <section class="resolution_section">
                    <div class="container">
                        <div>
                            <aura:if isTrue="{!v.isLead}">
                                
                                <aura:if isTrue="{!v.ClaimsDetails[0].TT_respond_evidence_gathering__c}">
                                    <lightning:button variant="brand" label="Review/edit evidence" title="Review/edit evidence"
                                                      onclick="{! c.showContinue}" class="see-all"/>
                                    <aura:set attribute="else">
                                        <lightning:button variant="brand" label="Submit evidence" title="Continue claim journey"
                                                          onclick="{! c.showContinue}" class="see-all"/>
                                    </aura:set>
                                </aura:if>
                                <lightning:button variant="brand" label="Agree with the agent/landlord’s claim" title="Agree with the agent/landlord’s claim"
                                                  onclick="{!c.doAgreeToClaim }" class="see-all"/>
                                <aura:set attribute="else">
                                    <lightning:button variant="brand" label="View evidence" title="View evidence"
                                                      onclick="{!c.showContinue}" class="see-all"/>
                                </aura:set>
                            </aura:if>
                        </div>
                        <img src="{!$Resource.TDSTheme  + '/assets/img/resolution.png'}" alt="Resolution" />
                    </div>
                </section>
                <!-- Resolution Section HTML End -->
                <div class="deposit-agent">
                    <div class="container">
                        <div class="row">
                            <div class="col-md-9">
                                <!-- Table HTML Start -->
                                <div class="table-container no_navigation ZD_custom_table">
                                    <table class="display scrollbar" style="width:100%">
                                        <tbody>
                                            <tr>
                                                <td>Address</td>
                                                <td>{!v.ClaimsDetails[0].Deposit_Account_Number__r.Property_Address__c}</td>
                                            </tr>
                                            <tr>
                                                <td>Claim status</td>
                                                <td>{!v.ClaimsDetails[0].Status}</td>
                                            </tr>
                                            
                                            <tr>
                                                <td>Tenancy start date</td>
                                                <td><lightning:formattedDateTime value="{!v.ClaimsDetails[0].Deposit_Account_Number__r.Start_Date__c}" year="numeric" month="numeric" day="numeric" /></td>
                                            </tr>
                                            <tr>
                                                <td>Guarantee number</td>
                                                <td>{!v.ClaimsDetails[0].ZD_Reference_Number__c}</td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                                <!-- Table HTML End -->
                                
                                <!-- Table HTML Start -->
                                <div class="table-container no_navigation no_space">
                                    <table id="example-2" class="display scrollbar dataTable no-footer" style="width:100%">
                                        <thead>
                                            <tr>
                                                <th>Name</th>
                                                <th>Type</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <aura:iteration var="caseParticpnt" items="{!v.ClaimsDetails[0].Case_Participants__r}" indexVar="i">
                                                <aura:if isTrue="{!caseParticpnt.Type__c =='Tenant'}">
                                                    
                                                    <aura:if isTrue="{!(i+1)%2 == 0}">
                                                        <tr class="even">
                                                            <td> {!caseParticpnt.Contact__r.Name} </td>
                                                            <aura:if isTrue="{!caseParticpnt.Is_Lead__c}">
                                                                <td> Lead tenant </td>
                                                                <aura:set attribute="else">
                                                                    <td> Joint tenant </td>
                                                                </aura:set>
                                                            </aura:if>
                                                            
                                                            <!-- <td> {!caseParticpnt.Type__c}  </td> -->
                                                        </tr>
                                                    </aura:if>
                                                    <aura:if isTrue="{!(i+1)%2 != 0}">
                                                        <tr class="odd">
                                                            <td> {!caseParticpnt.Contact__r.Name} </td>
                                                            <aura:if isTrue="{!caseParticpnt.Is_Lead__c}">
                                                                <td> Lead tenant </td>
                                                                <aura:set attribute="else">
                                                                    <td> Joint tenant </td>
                                                                </aura:set>
                                                            </aura:if>
                                                            <!-- <td> {!caseParticpnt.Type__c}  </td> -->
                                                        </tr>
                                                    </aura:if>
                                                </aura:if>
                                            </aura:iteration>
                                        </tbody>
                                    </table>
                                </div>
                                <!-- Table HTML End -->
                            </div>
                            <div class="col-md-3 info-right-tab">
                                <p>This page provides a summary of the claim, including where the claim is in the process.</p>
                                <p>Please progress to review the information and documents that your landlord/agent submitted, and provide the evidence that supports your position.</p>
                                <p>One tenant must act as lead tenant during the claim process. The lead tenant acts on behalf of the other named tenants, including uploading evidence.</p>
                                <p>We communicate with all tenants throughout the claim process, but only the lead tenant is able to make decisions on the claim.</p>
                                <p>If you wish to change the lead tenant, then please contact us via "Need More Help?" below.</p>
                                <p class="questionsmark"> <img src="{!$Resource.TDSTheme  + '/assets/img/question-circle.png'}" alt="Question Mark"/> <a href="#">Need More Help</a>.</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            
        </div>
    </aura:if>
    
    <!-- <div class="col-md-12" >      
        <h2 style="margin-bottom:5px">Submit evidence</h2>
    </div> -->
    <aura:if isTrue="{!v.isLead}">
        <aura:if isTrue="{!!v.ClaimsDetails[0].TT_respond_evidence_gathering__c}">
            <div class="container">
            <div class="row">
                <div class="col-md-9" style="margin-bottom:-30px">
                    <h2>Submit evidence</h2>
                </div>             
            </div>
        </div> 
        	<!--<h2 style="margin-bottom:5px" class="col-md-12">Submit evidence</h2>-->
            <aura:set attribute="else">
                <div class="container">
                    <div class="row">
                        <div class="col-md-9" style="margin-bottom:-30px">
                            <h2>Review/edit evidence</h2>
                        </div>             
                    </div>
                </div>               
                <!--<h2 style="margin-bottom:5px" class="col-md-12" >Review/edit evidence</h2>-->
        	</aura:set>
        </aura:if>
        <aura:set attribute="else">
            <h2 style="margin-bottom:5px">Review evidence</h2>
        </aura:set>
    </aura:if>
    <aura:if isTrue="{!v.ViewContinue}">
        <div class="tab-content custom-tab-content" id="sf-tabContent">
            
            <div class="tab-pane fade show active" id="sf-deposit" role="tabpanel" aria-labelledby="sf-deposit-tab">
                
                <div class="list-of-btns" id="scrollView">
                    <div class="container">
                        <lightning:button onclick="{!c.goTodepositsummary}" aura:id="div0" class="go-back"><img  src="{!$Resource.TDSTheme  + '/assets/img/md-arrow-dropleft.png'}" alt="Back Icon"/> Go back</lightning:button>
                        <aura:if isTrue="{!v.isLead}">
                            <lightning:button variant="brand" label="Cancel Dispute" title="Cancel Dispute"
                                              onclick="{!c.doAgreeToClaim }" class="see-all"/>
                        </aura:if>
                    </div>
                </div>
                
                <div class="deposit-agent" >
                    <div class="container">
                        <div class="row">
                            <div class="col-md-9">
                                <section class="deposit_steps">
                                    <div class="deposit_steps_detail">
                                        <h6>You have until <ui:outputDate format="DD/MM/YYYY" value="{!v.ClaimsDetails[0].Respond_Date__c}" />  to submit details of your case and your evidence.</h6>
                                     
                                        <!--Paid amount start-->
                                              <div class="claim-summary_gray-panel">
                                               <strong><p>We have released the following amounts in accordance with the parties' instructions</p></strong>
                                                  <br/>
                                                <div class="row">
                                                    <div class="col-lg-6 col-sm-9">
                                                        <p>Amount repaid to agent/landlord</p>
                                                    </div>
                                                    <div class="col-lg-6 col-sm-3">
                                                            <p><lightning:formattedNumber value="{!v.ClaimsDetails[0].Total_Agreed_by_Tenant__c}" style="currency"  minimumFractionDigits="2" maximumFractionDigits="2"/></p>
                                                    </div>
                                                </div>
                                                <div class="row my-4">
                                                    <div class="col-lg-6 col-sm-9">
                                                        <p >Amount repaid to tenant(s)</p>
                                                    </div>
                                                    <div class="col-lg-6 col-sm-3">
                                                        <p><lightning:formattedNumber value="{!v.ClaimsDetails[0].Agreed_amount_from_AGLL_to_TT__c}" style="currency"  minimumFractionDigits="2" maximumFractionDigits="2"/></p>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-lg-6 col-sm-9">
                                                        <p><b>Total amount repaid</b></p>
                                                    </div>
                                                    <div class="col-lg-6 col-sm-3">
                                                        <p><b> <lightning:formattedNumber value="{!v.ClaimsDetails[0].Agreed_amount_from_AGLL_to_TT__c + v.ClaimsDetails[0].Total_Agreed_by_Tenant__c}" style="currency" minimumFractionDigits="2" maximumFractionDigits="2"/></b></p>
                                                    </div>
                                                </div>
                                            </div>
                                            <!--Paid amount end-->
                                        
                                        <div class="claim-summary_gray-panel">
                                            <h5>Claim summary</h5>
                                            <div class="row">
                                                <div class="col-lg-6 col-sm-9">
                                                    <p>Amount requested by agent/landlord</p>
                                                </div>
                                                <div class="col-lg-6 col-sm-3">
                                                    <p><lightning:formattedNumber value="{!v.ClaimsDetails[0].Total_Agreed_by_AG_LL__c}" style="currency"  minimumFractionDigits="2" maximumFractionDigits="2"/></p>
                                                </div>
                                            </div>
                                            <div class="row my-4">
                                                <div class="col-lg-6 col-sm-9">
                                                    <p class="text-right">- Of which is agreed by you</p>
                                                </div>
                                                <div class="col-lg-6 col-sm-3">
                                                    <p>(<lightning:formattedNumber value="{!v.ClaimsDetails[0].Total_Agreed_by_Tenant__c}" style="currency"  minimumFractionDigits="2" maximumFractionDigits="2"/>)</p>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-lg-6 col-sm-9">
                                                    <p><b>Disputed amount - held by SafeDeposits</b></p>
                                                </div>
                                                <div class="col-lg-6 col-sm-3">
                                                    <p><b> <lightning:formattedNumber value="{!v.ClaimsDetails[0].Total_Agreed_by_AG_LL__c - v.ClaimsDetails[0].Total_Agreed_by_Tenant__c}" style="currency" minimumFractionDigits="2" maximumFractionDigits="2"/></b></p>
                                                </div>
                                            </div>
                                        </div>
                                        
                                        <div class="row">
                                            <div class="col-lg-12 col-md-12">
                                                <div class="agree-container">
                                                    <div class="agree-detail" style="display: block;">
                                                        <div class="form-group row">
                                                            <div class="col-sm-4">
                                                                <div class="request-section">
                                                                    <label>Requested by agent/landlord</label>
                                                                </div>
                                                            </div>
                                                            <div class="col-sm-4">
                                                                <div class="response-section">
                                                                    <label>Previously agreed by you</label>
                                                                </div>
                                                            </div>
                                                            <div class="col-sm-4">
                                                                <div class="dispute-section">
                                                                    <label>Amount in dispute</label>
                                                                </div>
                                                            </div>
                                                            
                                                            <aura:iteration var="desptItems" items="{!v.ClaimsDetails[0].Dispute_Items__r}">
                                                                <div class="col-sm-4">
                                                                    <div class="request-section">
                                                                        <aura:if isTrue="{!desptItems.Type__c=='Other'}">
                                                                            <div class="form-group">
                                                                                <label for="Others" class=" col-form-label">{!desptItems.Type__c}</label>
                                                                                <!-- <textarea rows="3" class="form-control" style="overflow: hidden;" disabled="true" id="Others">{!'£'+desptItems.Agreed_by_AGLL__c}&nbsp;
                                                                                    {!desptItems.Other_Reason__c}</textarea> -->
                                                                                <div id="textarea"  contenteditable="true" disabled="true"  >
                                                                                    <div style="padding:2px; padding-right: 10px;">
                                                                                        <p id="ptag" disabled="true" > <lightning:formattedNumber value="{!desptItems.Agreed_by_AGLL__c}" style="currency"  minimumFractionDigits="2" maximumFractionDigits="2"/></p>
                                                                                        <p  id="ptag"  disabled="true" >{!desptItems.Other_Reason__c}</p>
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                            <aura:set attribute="else">
                                                                                <div class="form-group">
                                                                                    <label for="Cleaning" class=" col-form-label">{!desptItems.Type__c}</label>
                                                                                    <lightning:input type="number" name="input1"  formatter="currency" step="0.01"  disabled="true"  value="{!desptItems.Agreed_by_AGLL__c}" />
                                                                                    <!-- <input type="number" class="form-control" disabled="true" id="Cleaning" placeholder="{!'£'+desptItems.Agreed_by_AGLL__c}"/> -->
                                                                                </div>
                                                                            </aura:set>
                                                                        </aura:if>
                                                                    </div>
                                                                </div>
                                                                <div class="col-sm-4">
                                                                    <div class="response-section">
                                                                        <div class="form-group">
                                                                            <label class="col-form-label">{!desptItems.Type__c}</label>
                                                                            <lightning:input type="number" name="input1"  formatter="currency" step="0.01"  disabled="true"  value="{!desptItems.Agreed_by_Tenant__c}" />
                                                                            <!--<input type="number" disabled="true" class="form-control" placeholder="{!'£'+desptItems.Agreed_by_Tenant__c}"/> -->
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div class="col-sm-4">
                                                                    <div class="dispute-section">
                                                                        <div class="form-group">
                                                                            <label class="col-form-label">{!desptItems.Type__c}</label>
                                                                            <lightning:input type="number" name="input1"  formatter="currency" step="0.01"  disabled="true"  value="{!sub(desptItems.Agreed_by_AGLL__c,desptItems.Agreed_by_Tenant__c)}" />
                                                                            <!--  <input type="text" disabled="true" class="form-control" placeholder="{!'£'+sub(desptItems.Agreed_by_AGLL__c,desptItems.Agreed_by_Tenant__c)}"/> -->
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </aura:iteration>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div> 
                                        
                                        <aura:if isTrue="{!v.ClaimsDetails[0].Claim_exceed__c=='Yes'}">
                                            <div class="form-group row">
                                                <label class="col-sm-4 col-form-label">Total claim amount</label>
                                                <div class="col-sm-8">
                                                    <p>The agent/landlord has indicated that their total claim exceeds the deposit amount and have given the following breakdown. You can respond to individual claim areas in the next page(s). The maximum the adjudicator can award is the deposit amount.</p>
                                                    
                                                    <div class="form-group">
                                                        <lightning:textarea disabled="true" class=" topText" value="{!v.ClaimsDetails[0].Claim_exceeds_comment_AGLL__c}"  name="input1"  />
                                                    </div>
                                                </div>
                                            </div>
                                        </aura:if>
                                        
                                        <h6>Please give your consent to the following:</h6>
                                        
                                        <p>I give authority to SafeDeposits Scotland to consider my dispute and confirm that:</p>
                                        
                                        <ul class="custom-list">
                                            <li>I accept responsibility for ensuring that any personal data contained in the information I submit complies with the Data Protection Act 1988 and the General Data Protection Regulation.</li>
                                            <li>I consent to the information I provide being shared with the other party to the dispute, the adjudicator and anyone else involved in resolving it.</li>
                                            <li>I agree that the decision of the adjudicator will be final and binding, subject to the review process.</li>
                                        </ul>
                                        <aura:if isTrue="{!v.isLead}">
                                           <div class="form-row">
                                                <div class="form-group col-md-12 text-right slds-grid slds-gutters">
                                                    <div class="form-check slds-col"></div>
                                                    <div class="form-check slds-col">
                                                        <input type="checkbox" checked="{!v.consent}" class="form-check-input" id="exampleCheck2" onchange="{!c.consentSelected}"/>
                                                        <label class="form-check-label" for="exampleCheck2">I consent to the above</label>
                                                        
                                                    </div>
                                                    <div class="form-check slds-col"  title = "If you withdraw consent from the dispute, SafeDeposits Scotland will pay the deposit as requested by your agent/landlord in 30 working days. You will then need to resolve your dispute independent of the scheme. You can change your mind at any point within 30 working days by contacting SafeDeposits Scotland.">
                                                        <input type="checkbox" value="{!v.noconsent}" class="form-check-input" id="exampleCheck3" onchange="{!c.consentSelected}"/>
                                                        <label class="form-check-label" for="exampleCheck3">I do not consent</label>
                                                    </div>
                                                </div>
                                                <div class="form-group col-md-12 text-right slds-grid slds-gutters">
                                                    <aura:if isTrue="{!v.showConsentError}">
                                                        <p class="error-msg text-right">
                                                            <!--You must give your authority for the dispute to be considered. -->
                                                            {!v.tenantgathErrorNew}
                                                        </p>
                                                    </aura:if>
                                                </div>
                                            </div>
                                        </aura:if>
                                        <div class="text-right">
                                            <lightning:button variant="brand" label="Continue" onclick="{!c.goToClaimBreakdown}" class="see-all"/>
                                        </div>
                                    </div>
                                </section>
                                
                            </div>
                            <div class="col-md-3 info-right-tab">
                                <p>This is your opportunity to respond to the claim made by the agent/landlord . You can return to edit your response at any time up <strong><ui:outputDate format="DD/MM/YYYY" value="{!v.ClaimsDetails[0].Respond_Date__c}" /></strong>.</p>
                                <p>During the process, your submission will be saved automatically each time you press ‘Continue’.  If there is more than one tenant involved in the tenancy, only the tenant taking part in the repayment process (the lead tenant) can provide a response on behalf of all the tenants. The lead tenant is listed on the previous page.</p>
                                
                                <p>After <strong><ui:outputDate format="DD/MM/YYYY" value="{!v.ClaimsDetails[0].Respond_Date__c}" /></strong>, the agent/landlord will be able to view your submission and the adjudicator will use all the information from both parties in making their decision.It is important that you provide the information you wish the adjudicator to see now. Safedeposits will generally not contact you again to ask for more information.</p>
                                
                                <p>Please respond fully to all the questions asked. It is your responsibility to explain your position clearly and highlight the relevant evidence.</p>
                                
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </aura:if>
    <aura:if isTrue="{!v.showClaimBreakdown}">
        <div class="tab-content custom-tab-content" id="sf-tabContent">
            
            <div class="tab-pane fade show active" id="sf-deposit" role="tabpanel" aria-labelledby="sf-deposit-tab">
                <div class="list-of-btns">
                    <div class="container">
                        <lightning:button onclick="{!c.goToCliamSummary}" aura:id="div2" class="go-back">
                            <img  src="{!$Resource.TDSTheme  + '/assets/img/md-arrow-dropleft.png'}" alt="Back Icon"/> Go Back
                        </lightning:button>
                        <aura:if isTrue="{!v.isLead}">
                            <lightning:button variant="brand" label="Cancel Dispute" title="Cancel Dispute"
                                              onclick="{!c.doAgreeToClaim }" class="see-all"/>
                        </aura:if>
                    </div>
                    
                </div>
                <div class="deposit-agent" >
                    
                    <div class="container" id="scrollView">
                        <div class="row">
                            <div class="col-md-9">
                                
                                <section class="deposit_steps">
                                    
                                    <div class="deposit_steps_count">
                                        <div class="state-indicator">
                                            <span class="circle">1</span>
                                            <p>Key documents</p>
                                        </div>
                                        <div class="state-indicator active">
                                            <span class="circle">2</span>
                                            <p>Claim breakdown</p>
                                        </div>
                                        <div class="state-indicator">
                                            <span class="circle">3</span>
                                            <p>Additional comments</p>
                                        </div>
                                        <div class="state-indicator">
                                            <span class="circle">4</span>
                                            <p>Review submission</p>
                                        </div>
                                    </div>
                                    
                                    <div class="deposit_steps_detail" >
                                        
                                        <form>
                                            <aura:iteration var="desptItems" items="{!v.ClaimsDetails[0].Dispute_Items__r}" indexVar="i">
                                                <aura:if isTrue="{!v.currentItem==i}">
                                                    
                                                    <aura:if isTrue="{!desptItems.Type__c=='Cleaning'}">
                                                        <aura:if isTrue="{!v.agentResponse}">
                                                            <h6 class="blue-clr" id="scrollView">Agent/landlord’s evidence for their claim for <span class="lowercase">{!desptItems.Type__c}</span></h6>
                                                            <div class="row">
                                                                <div class="col-lg-12 col-md-12">
                                                                    <div class="agree-container">
                                                                        <div class="agree-detail" style="display: block;">
                                                                            <div class="form-group row">
                                                                                <div class="col-sm-4">
                                                                                    <div class="request-section">
                                                                                        <label>Requested by agent/landlord</label>
                                                                                        <div class="form-group">
                                                                                            <lightning:input type="number" name="input1"  formatter="currency" step="0.01"  disabled="true"  value="{!desptItems.Agreed_by_AGLL__c}" />
                                                                                        </div>
                                                                                    </div>
                                                                                </div>
                                                                                <div class="col-sm-4">
                                                                                    <div class="response-section">
                                                                                        <label>Previously agreed by you</label>
                                                                                        <div class="form-group">
                                                                                            <lightning:input type="number" name="input1"  formatter="currency" step="0.01"  disabled="true"  value="{!desptItems.Agreed_by_Tenant__c}" />
                                                                                        </div>
                                                                                    </div>
                                                                                </div>
